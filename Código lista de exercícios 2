void setup()
{
  pinMode(4, OUTPUT);
  pinMode(3, OUTPUT);
}

void loop()
{
  
    for(int contadorVermelho = 0; contadorVermelho < 10 ; contadorVermelho++)
  {
  PiscaLedVermelho();
}
  
}

  void PiscaLedVermelho()
    
   
{
    
digitalWrite(4, HIGH);
  delay(1000);
  digitalWrite(4, LOW);
  delay(1000);
  
digitalWrite(3, HIGH);
  delay(1000);
  digitalWrite (3, LOW);
  delay(1000);
}
